<?xml version="1.0" encoding="UTF-8"?>
<params>
	<ostypes>
	  <windowsType>1</windowsType>
	  <linuxType>2</linuxType>
	</ostypes>
	<photoDatabase>
		<fileName>photos_master.db</fileName>
		<tables>
			<photoTable>
				<Name>Photos</Name>
				<Columns>
					<photoKey>photo_key</photoKey>
					<photoFile>photo_file</photoFile>
					<photoDate>photo_date</photoDate>
					<photoYear>taken_year</photoYear>
					<photoMonth>taken_month</photoMonth>
					<photoDay>taken_day</photoDay>
					<photoHour>taken_hour</photoHour>
					<photoMinute>taken_minute</photoMinute>
					<photoGMT>taken_timezone</photoGMT>
					<modifyDate>modification_date</modifyDate>
					<rootDirNum>root_dir_num</rootDirNum>
					<insertDate>inserted_date</insertDate>
					<houseNum>geoHouseNum</houseNum>
					<street>geoStreetName</street>
					<city>geoCityName</city>
					<state>geoStateName</state>
					<postcode>geoPostcode</postcode>
					<country>geoCountryName</country>
					<lat>picLatitude</lat>
					<long>picLongitude</long>
				</Columns>
			</photoTable>
			<peopleTable>
				<Name>People</Name>
				<Columns>
					<peopleKey>people_key</peopleKey>
					<personName>person_name</personName>
				</Columns>
			</peopleTable>
			<photoLinkerTable>
				<Name>PhotoLinker</Name>
				<Columns>
					<linkerPeople>person</linkerPeople>
					<linkerPhoto>photo</linkerPhoto>
				</Columns>
			</photoLinkerTable>
			<rootTable>
				<Name>RootDirs</Name>
				<Columns>
					<rootKey>directory_root_key</rootKey>
					<windowsRootPath>root_path_windows</windowsRootPath>
					<linuxRootPath>root_path_linux</linuxRootPath>
				</Columns>
			</rootTable>
			<metadataTable>
				<Name>Metadata</Name>
				<Columns>
					<metadataName>item_name</metadataName>
					<metadataValue>item_value</metadataValue>
				</Columns>
				<Fields>
					<metadataLastEditedField>last_edited_date</metadataLastEditedField>
				</Fields>
			</metadataTable>
			<commentLinkerUserTable>
				<Name>UserCommentLinker</Name>
				<Columns>
					<commentLinkerPhoto>photo_number</commentLinkerPhoto>
					<commentLinkerTag>tag</commentLinkerTag>
					<commentLinkerTagProbability>tag_probability</commentLinkerTagProbability>
				</Columns>
			</commentLinkerUserTable>
			<commentLinkerGoogleTable>
				<Name>GoogleCommentLinker</Name>
				<Columns>
					<commentLinkerPhoto>photo_number</commentLinkerPhoto>
					<commentLinkerTag>tag</commentLinkerTag>
					<commentLinkerTagProbability>tag_probability</commentLinkerTagProbability>
				</Columns>
			</commentLinkerGoogleTable>
			<commentLinkerClarifaiTable>
				<Name>ClarifaiCommentLinker</Name>
				<Columns>
					<commentLinkerPhoto>photo_number</commentLinkerPhoto>
					<commentLinkerTag>tag</commentLinkerTag>
					<commentLinkerTagProbability>tag_probability</commentLinkerTagProbability>
				</Columns>
			</commentLinkerClarifaiTable>
		</tables>
	</photoDatabase>
	<visionTaggingParams>
		<googleTagging>
			<googVisionLabelPrefix>googVision_</googVisionLabelPrefix>
			<googImageHistoryPrefix>Google added: </googImageHistoryPrefix>
		</googleTagging>
		<clarifaiTagging>
			<clarifaiVisionLabelPrefix>clarifaiVision_</clarifaiVisionLabelPrefix>
			<clarifaiImageHistoryPrefix>Clarifai added: </clarifaiImageHistoryPrefix>
		</clarifaiTagging>
		<database>
			<fileName>visionDatabase.db</fileName>
			<tables>
				<visionMetaTable>
					<Name>Variables</Name>
					<Columns>
						<NameColumn>Name</NameColumn>
						<ValueColumn>Value</ValueColumn>
					</Columns>
				</visionMetaTable>
				<recordDataTable>
					<Name>visionData</Name>
					<Columns>
						<File>fileName</File>
						<Checked>lastCheckedDate</Checked>
						<Portrait>readAsPortrait</Portrait>
						<Source>machineVisionSource</Source>
						<Valid>validated</Valid>
					</Columns>
				</recordDataTable>
			</tables>
			<Fields>
				<googFields>
					<ReadsThisMonth>Reads_performed_this_month_google</ReadsThisMonth>
					<ReadsPerMonth>Reads_per_month_google</ReadsPerMonth>
					<NewMonthDate>New_month_date_google</NewMonthDate>
					<DayLastRead>Day_last_read_google</DayLastRead>
					<DayOfNewMonth>Day_of_new_month_google</DayOfNewMonth>
				</googFields>
				<clarifaiFields>
					<ReadsThisMonth>Reads_performed_this_month_clarifai</ReadsThisMonth>
					<ReadsPerMonth>Reads_per_month_clarifai</ReadsPerMonth>
					<NewMonthDate>New_month_date_clarifai</NewMonthDate>
					<DayLastRead>Day_last_read_clarifai</DayLastRead>
					<DayOfNewMonth>Day_of_new_month_clarifai</DayOfNewMonth>
				</clarifaiFields>
			</Fields>
		</database>
	</visionTaggingParams>
	<serverParams>
		<geoServerPort>8040</geoServerPort>
		<displayServerPort>8001</displayServerPort>
		<successVal>Success</successVal>
	</serverParams>
</params>
